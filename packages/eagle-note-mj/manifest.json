{
  "name": "Eagle Note MJ",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Uses the chrome.scripting API to inject JavaScript into web pages.",
  "permissions": ["scripting", "webNavigation"],
  "host_permissions": ["https://example.com/*"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://www.midjourney.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  }
}
